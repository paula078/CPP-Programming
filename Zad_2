#include <iostream>
#include <cmath>
using namespace std;

int area(int a);
int area(int a, int b);
double area(double r);

int main()
{
  int side;
  int oneSide;
  int twoSide;
  double r;

  cin >>side;
  cout<<"Pole kwadratu o boku "<<side<<" = "<<area(side)<<endl;

  cin >>oneSide >> twoSide;
  cout <<"Pole prostokata o bokach: " <<oneSide<<", "<<twoSide<< " = " <<area(oneSide,twoSide)<<endl;

  cin >>r;
  cout<< "Pole kola o promieniu: "<<r<<" = "<<area(r);
  return 0;
}

int area(int a){
return a*a;
}

int area(int a, int b){
return a*b;
}

double area(double r){
return 3.14*r*r;
}
