#include <iostream>
using namespace std;
int main(){

    int A [5];
    int B[5];
    int b=4;

    for(int i=0 ; i<5 ; i++) {

        A[i] = 2*i+1;
        cout << A[i] << endl;
    }
    
    cout << "Odwrocona: " << endl;

    for(int i=0 ; i<5 ; i++){
        B[i]=A[b];
        cout << B[i] << endl;
        b--;
    }
}
