
#include <iostream>
using namespace std;

int main(){
float score;

cout << "Enter your score <0,100>: ";
cin >> score;

if (score>=90){
    cout << "Your grade is 5";
}
else if(score>=80){
    cout<< "Your grade is 4.5";
}
else if(score>=70){
    cout << "Your grade is 4";
}
else if (score>=60){
    cout << "Your grade is 3.5";
}
else if (score>=50){
    cout << "Your grade is 3";
}
else { cout << "You failed";}

}
