#include <iostream>
#include <time.h>
using namespace std;

int main(){
int n;

cout << "Podaj liczbe naturalna <=20:"<<endl;
cin>>n;
cout << endl;

int *tab = new int[n];

for(int i=0; i<n; i++){
    cin >>*(tab+i);
}
for(int i=0; i<n; i++){
    if (*(tab+i)<0)
        *(tab+i)=0;
}
cout << endl;
for(int i=0; i<n; i++){
    cout << *(tab+i) <<endl;
}
delete []tab;
return 0;
}
