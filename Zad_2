#include <iostream>
#include <string>
#include <cmath>
using namespace std;

string reverse(string s){
int len;
int tmp;
len=s.length();

for(int i=0; i<len/2; i++){
    tmp=s[i];
    s[i]=s[len-1-i];
    s[len-1-i]=tmp;
}
return s;
}

int main(){
string a;

a="slowo";

cout << reverse(a);

return 0;
}
