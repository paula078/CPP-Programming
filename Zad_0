#include <iostream>
#include <cmath>
using namespace std;

bool friends(int smaller, int div );
int main()
{
int a;
int b;

    cout<<"Podaj dwie liczby"<<endl;
    cin>> a >> b;

    cout << friends (a,b);

    return 0;
}

bool friends(int a, int b ){
//obliczamy sumę dzielników
int a_sum=0;
int b_sum=0;

for(int i=1;i<b;i++){
    if(b%i==0){
     b_sum+=i;
    }
}
for(int i=1;i<a;i++){
    if(a%i==0){
     a_sum+=i;
    }
}

if(b==a_sum && a==b_sum)
    return 1;
else
    return 0;
}
