#include <iostream>
#include <string>
#include <cmath>
using namespace std;

string changebase(string s, int base1, int base2){

string wynik;
int length=s.length();
char liczba[length+1];
int tmp=0;
int sum=0;
int p=1;     //potega zerowa

//DowolnaNaDecymalna

//Zamieniam liczbe na chary
for(int i=0; i<length; i++)
        liczba[i]=s[i];

for(int i=length-1; i>=0; i--){
        if(s[i]>='0'&& s[i]<='9'){
            tmp+=(liczba[i]-'0')*p;
            p*=base1;
        }
		else if(s[i]>='A'&& s[i]<='F'){
            tmp+=(liczba[i]-55)*p;
            p *= base1;

        }
}
//---------------------------------
//DecymalnaNaDowolna
//tmp = nasz wynik dziesietny

char tab[50];  //przechowuje ostateczny wynik

int cnt =0;
while(tmp!=0){
int reszta=0;
    reszta=tmp%base2;
    if(reszta<10){
        tab[cnt]= reszta+'0';
        cnt++;
    }
    else{
        tab[cnt]=reszta+55;
        cnt++;
    }
    tmp/=base2;
}

for(int j=cnt-1;j>=0;j--)
	wynik+=tab[j];

return wynik;
}

int main(){

string liczba="4F";
int base1=16;
int base2=8;

cout <<changebase(liczba,base1,base2);

return 0;
}
