#include <time.h>
#include <windows.h>
#include <iostream>
using namespace std;

int main(){

int B[12];
int maxi=0;

srand(time(NULL));
for(int i=0; i<12; i++ )
{
B[i] = rand()%10+1;

cout << "B[" <<i+1<<"]="<< B[i] << endl;

}
for(int i=0; i<12; i++ ){
if(B[i]>maxi)
{
    maxi=B[i];
}
}
cout << "Najwieksza liczba z tabeli to: " << maxi;
}
