#include <iostream>
#include<time.h>
using namespace std;

int main(){
int size_1=0;
int size_2=0;
int size_3=0;

cin >>size_1;
cin >>size_2;
cin >>size_3;

srand(time(NULL));

int ***A = new int**[size_1];

for(int i=0; i<size_1; i++){
*(A+i)= new int*[size_2];

    for(int j=0; j<size_2; j++){
    *(*(A+i)+j)=new int[size_3];

        for(int k=0; k<size_3; k++){
        *(*(*(A+i)+j)+k)=rand()%300;

cout <<"["<<i+1<<"]"<<"["<<j+1<<"]"<<"["<<k+1<<"]"<<"="<< *(*(*(A+i)+j)+k)<<endl;
}
}
}
for(int i=0; i<size_1; i++){
    for(int j=0; j<size_2; j++){
            delete []*(*(A+i)+j);
    }
}
        for(int i=0; i<size_1; i++)
            delete []*(A+i);

delete []A;
return 0;
}
