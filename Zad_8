#include <iostream>
#include <time.h>
using namespace std;

int main(){
int tmp;
int n;
cin>> n;
cout<< endl;
srand(time(NULL));
int *A = new int[n];

for(int i=0; i<n; i++){
*(A+i)=rand()%11;
cout << *(A+i)<<"   ";
}
cout <<"\nZamieniona kolejnosc:" <<endl;
for(int i=0; i<n/2; i++){
    tmp=*(A+i);
    *(A+i)=*(A+(n-1-i));
    *(A+(n-1-i))=tmp;
}
for(int i=0; i<n; i++)
cout << *(A+i)<<"   ";

delete []A;
return 0;
}
