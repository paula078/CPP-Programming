#include <iostream>
#include <cmath>
using namespace std;

int main(){
float a,b,c;
float delta;
float x1, x2;

cout << "Enter coefficients a , b , c:";
cin >> a >> b >> c;

delta= b*b-4*a*c;

if(delta>0){

x1=((-b+sqrt(delta))/(2*a));
x2=((-b-sqrt(delta))/(2*a));

    cout << "There are two real roots: "<< x1 <<" and " << x2 ;
}
else if(delta<0){
    cout << "There is no real root";
}
else{
    x1=-b/(2*a);
    cout << "There is only one real root: " << x1;
}
}

