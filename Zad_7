#include <iostream>
#include <time.h>
using namespace std;

int main(){
int maxi=0;
int n;
cin>> n;
cout<< endl;
srand(time(NULL));
int *A = new int[n];

for(int i=0; i<n; i++)
*(A+i)=rand()%11;

for(int i=0; i<n; i++)
    cout << *(A+i) <<endl;

for(int i=0; i<n; i++){
    if (*(A+i)>maxi)
        maxi=*(A+i);
}
cout << "Max:"<< maxi;

delete []A;
return 0;
}
