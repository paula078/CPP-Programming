
#include <iostream>
using namespace std;

int main() {

   int n;
   int a=0;
   int b=1;
    int x;
    cout << "Enter n ( n>0 ): ";
    cin >> n;

    if(n==1){
        cout<< a;
    }
    else if(n==2) {
        cout << a << " " << b;
    }
    else{
        cout << a << " " << b << " ";
        for (int i=0; i<n-2; i++)
        {
            x=a+b;
            a=b;
            b=x;


            cout<< x <<" ";

        }
    }

return 0;
}
