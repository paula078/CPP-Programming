#include <iostream>
#include <time.h>
using namespace std;

int main()
{
int sum=0;
int C[7][7];
srand(time(NULL));

for(int i=0; i<7; i++){
        for(int j=0; j<7; j++)
    C[i][j]=rand()%21-10;
}
for(int i=0; i<7; i++){
        for(int j=0; j<7; j++)
    cout << C[i][j] <<"\t";
    cout << "\n";
}
for(int i=0; i<7; i++){
        for(int j=0; j<7; j++){
sum+=C[i][j];
        }
}
cout << "Sum = " << sum;
return 0;
}
