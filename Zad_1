#include <iostream>
#include <time.h>
using namespace std;

int main(){
int n;

cout << "Podaj liczbe naturalna <=20:"<<endl;
cin>> n;
cout << endl;

int *tab = new int[n];
*tab=2;
for(int i=1; i<n; i++){
    *(tab+i)=*(tab+(i-1))*2;
}
for(int i=0; i<n; i++){
    cout<< *(tab+i)<<endl;
}
delete []tab;
return 0;
}
