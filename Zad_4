#include <iostream>
#include <time.h>
using namespace std;

int main(){

int n=7;
int C[n][n];
int x;
srand(time(NULL));

for(int i=0; i<n; i++){
        for(int j=0; j<n; j++)
        C[i][j]=rand()%21-10;
}
for(int i=0; i<n; i++){
        for(int j=0; j<n; j++)
            cout << C[i][j] <<"\t";
        cout << "\n";
}
 cout << "Transponowana: " <<endl;

for(int i=0; i<n; i++){
        for(int j=i+1; j<n; j++){
            x = C[i][j];
            C[i][j]=C[j][i];
            C[j][i]=x;
}
}
for(int i=0; i<n; i++){
        for(int j=0; j<n; j++){
    cout << C[i][j] <<"\t";
        }
    cout << "\n";
}
return 0;
}
