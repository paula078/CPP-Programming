#include <iostream>
#include <cmath>
using namespace std;
int main(){

int n;    //ostatnia cyfra przedzialu

cout << "Podaj n:" ;
cin >> n;

int *tab = new int [n];

for (int i=0;i<=n;i++)
{
tab[i]=i;
}

for (int i=2;i<=sqrt(n);i++){
          if(tab[i]!=0){
    for (int j=i*i;j<=n;j+=i)
        tab[j]=0;
}
}
cout << "Liczby pierwsze z przedzialu:";
for (int i=2;i<=n;i++)
{
    if(tab[i]!=0)
cout << tab[i] << " ";

}
}
