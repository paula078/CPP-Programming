#include <iostream>
#include <time.h>
using namespace std;

int main(){
int n;
int sum;
cout << "Podaj liczbe naturalna <=20:"<<endl;
cin>> n;
cout << endl;

int *tab = new int[n];

for(int i=0; i<n; i++){
    cin >>*(tab+i);
    sum+=*(tab+i);
    if(sum>100)
    {
        for(int j=i+1; j<n; j++){
            *(tab+j)=0;
        }
        break;
    }
}
cout << endl;
for(int i=0; i<n; i++)
{
    cout << *(tab+i) <<endl;
}
delete []tab;
return 0;
}
