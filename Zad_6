#include <iostream>
#include <cmath>
using namespace std;

int main(){
float x1,y1;
float x2,y2;
float r1,r2;
float sumap; //sum of radius
float dis;   //distance between the centres

cout << "Enter coordinates x, y of circle 1:" <<endl;
cin >> x1 >> y1;

cout << "Enter coordinates x, y of circle 2:" <<endl;
cin >> x2 >> y2;

cout << "Enter radius of circle 1 and of circle 2:" <<endl;
cin >> r1 >>r2;

dis= sqrt((x2-x1)*(x2-x1)+(y2-y1)*(y2-y1));

sumap=r1+r2;

if(sumap>=dis){
    cout << "Circles have non-empty intersection ";
}
else{ cout << "Circles have empty intersection";}
}
