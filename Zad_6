#include <iostream>
#include <time.h>
using namespace std;

int main(){
int tab[20][20][3];
srand(time(NULL));

for(int i=0; i<20; i++){
    for(int j=0; j<20; j++){
        for(int k=0; k<3; k++){

            tab[i][j][k]=rand()%256;
        }
    }
}
for(int k=0; k<3; k++){
        cout << endl;
        cout << endl;
    for(int i=0; i<20; i++){
        for(int j=0; j<20; j++){
        cout << tab[i][j][k] << "  ";
    }
cout << endl;
}
}
return 0;
}
